<h1>New Project</h1>
<%= form_for(@project) do |f| %>
	
	<p>
		<%= f.label :name, "Project Name" %><br>
		<%= f.text_field :name %>
	</p>

	<p>
		<%= f.label :description, "Project Description" %><br>
		<%= f.text_area :description %>
	</p>

	<%= f.submit %>
<% end %>

<br>

<%= link_to "Back", root_path %>